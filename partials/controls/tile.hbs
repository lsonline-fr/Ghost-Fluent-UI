<div class="list-cell ms-Grid-col ms-sm12 ms-lg6 ms-xl4 ms-xxl3" role="presentation">
    <div>
        <div class="item-tile" role="listitem">
            <a href="{{url}}" title="{{name}}" aria-label="{{name}}">
                <div class="thumbnail" style="background-color: {{accent_color}};">
                    {{#if feature_image}}
                    <img src="{{img_url feature_image}}" role="presentation" alt="" class="ms-fadeIn400" />
                    {{/if}}
                </div>
                <div class="tilePlateArea">
                    <div title="{{meta_title}}" class="tileText">{{name}}</div>
                </div>
            </a>
        </div>
    </div>
</div>